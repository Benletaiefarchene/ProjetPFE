{% block body %}
<!-- Titlebar
================================================== -->
<div id="titlebar" class="gradient">
	<div class="container">
		<div class="row">
			<div class="col-md-12">

				<h2>{% trans %} New Password {% endtrans %}</h2>

				<!-- Breadcrumbs -->
			

			</div>
		</div>
	</div>
</div>
<div class="container">
	<div class="row">
		<div class="col-xl-5 offset-xl-3">


			<div class="login-register-page">
				<!-- Welcome Text -->
				<div class="welcome-text">
                <h1>{% trans %}Enter your new password {% endtrans %}</h1>
					<form method="Post">
                    <label for="pass">{% trans %}Password {% endtrans %}</label>
                    <input type="password" name="password" id="pass">
                    <input type="hidden" name="token" value="{{token}}">
                    <button class="button full-width button-sliding-icon ripple-effect margin-top-10" type="submit" >{% trans %} validate {% endtrans %}<i class="icon-material-outline-arrow-right-alt"></i></button>
                    
                    </form>
				</div>
            </div>
        </div>
    </div>
</div>
{% endblock %}

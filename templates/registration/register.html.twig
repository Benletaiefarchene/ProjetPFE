{% block body %}
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script> 
	<div id="titlebar" class="gradient">
		<div class="container">
			<div class="row">
				<div class="col-md-12">

					<h2>{% trans %}Register {% endtrans %}</h2>

					<!-- Breadcrumbs -->
					<nav id="breadcrumbs" class="dark">
						<ul>
							<li>
								<a href="#">{% trans %} Home {% endtrans %}</a>
							</li>
							<li>{% trans %} Register {% endtrans %}</li>
						</ul>
					</nav>

				</div>
			</div>
		</div>
	</div>
	<div class="container">
		<div class="row">
			<div class="col-xl-5 offset-xl-3">

				<div
					class="login-register-page">
					<!-- Welcome Text -->
					<div class="welcome-text">
						<h3 style="font-size: 26px;">{% trans %} Let's create your account! {% endtrans %}</h3>
						<span>{% trans %} Already have an account? {% endtrans %}
							<a href="{{path('app_login')}}">{% trans %} Log In! {% endtrans %}</a>
						</span>
					</div>

					{{ form_start(registrationForm) }}

					
				
					
					{{ form_widget(registrationForm.email,{'attr':{'type':'text','class':'form-control','placeholder':'email...'}}) }}
					{{ form_widget(registrationForm.nom,{'attr':{'type':'text','class':'form-control','placeholder':'nom...'}}) }}
					{{ form_widget(registrationForm.prenom,{'attr':{'type':'text','class':'form-control','placeholder':'prenom...'}}) }}
					{{ form_widget(registrationForm.plainPassword, {'attr':{'type':'password','class':'form-control','placeholder':'password...'}}) }}
					
					<div class="submit-field"></div>
				</div>
				<button type="submit" class="button full-width button-sliding-icon ripple-effect margin-top-10">{% trans %} Register {% endtrans %}<i class="icon-material-outline-arrow-right-alt"></i>
				</button>
				{% if app.request.get('role')=='ROLE_CANDIDAT' %}
				<hr>
					<h3 style="text-align:center;">{% trans %} OR {% endtrans %}</h3>
					<hr>
					<div class="text-center social-btn">
					
            		<a href="{{path('connect_facebook_start')}}" class="btns btn-primary"><i class="fa fa-facebook"></i>&nbsp; Facebook</a>
					<a href="{{path('connect_google_start')}}" class="btns btn-danger"><i class="fa fa-google"></i>&nbsp; Google</a>
					{% elseif app.request.get('role')=='ROLE_RECRUTEUR' %}
					
					 {% endif %}
				</div>
				<hr>
				
				{{ form_end(registrationForm) }}

			</div>

		</div>
	</div>
</div>{% endblock %}

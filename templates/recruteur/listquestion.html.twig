	<div class="container">
		<div class="row">
			<div class=" col-lg-12">
			
			
				{% for Form in Form %}
					<div
						class="listings-container margin-top-35">

						<!-- Job Listing -->
						<a
							href="{{ path('detailedquestion',{'id': Form.id}) }}" class="job-listing">

